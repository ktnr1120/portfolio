{{!-- <h1>Hello Template Engine</h1> --}}

{{!-- <enctype="multipa...data">複数のデータを取り扱える指定 --}}
<form action="/" method="post" enctype="multipart/form-data">
    <h3>画像アップロードする</h3>
    <input type="file" name="imageFile" accept="image/*" />
    <input type="submit" />
</form>

<div>
    {{#each rows}}
    <p>↓に保存された画像が出力されます。</p>
    <img src={{this.imageName}} alt="" />
    <p>{{this.imageName}}</p>
    {{/each}}
</div>